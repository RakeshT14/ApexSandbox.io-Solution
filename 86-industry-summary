public Map<String, Decimal> industrySummary(List<Account> accounts) {
    //code here
    List<AggregateResult> annualRevenueSumList = [Select Sum(AnnualRevenue) annualRevenue,Industry from Account GROUP BY Industry];
    Map<String,Decimal> industryMap = new Map<String,Decimal>();
    for(AggregateResult agg:annualRevenueSumList){
        String accountIndustry = String.valueOf(agg.get('Industry')); 
        if(!industryMap.containsKey(accountIndustry)){
            Decimal accountAnnualRevenue = agg.get('annualRevenue');
            industryMap.put(accountIndustry,accountAnnualRevenue);
        }
    }
    return industryMap;

}