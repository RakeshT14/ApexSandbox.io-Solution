public Boolean isPrime(Integer num) {
    //code here
    if(num==2)
    {
        return true;
    }
    if(Math.mod(num,2)==0)
    {
        return false;
    }
    if(num==1)
    {
        return false;
    }
    for(Integer i=3;i<num;i++)
    {
        if(Math.mod(num,i)==0)
        {
            return false;
        }
    }
    return true;
}