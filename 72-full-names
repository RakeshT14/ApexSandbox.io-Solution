public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
    //code here
    List<String> fullNamesResult = new List<String>();
    for(Integer i=0;i<firstNames.size();i++)
    {
        String str = firstNames[i] +' '+ lastNames[i];
        fullNamesResult.add(str);
    } 
    return fullNamesResult;
}