public List<Integer> fibonacciSeries(Integer n) {
    //code here
    List<Integer> fiboNumbers = new List<Integer>();
    if(n==2)
    {
        fiboNumbers.add(1);
        fiboNumbers.add(1);
        return fiboNumbers;
    }else if(n==1)
    {
        fiboNumbers.add(1);
        return fiboNumbers;  
    }
    Integer a=1,b=1,i=0,sum=0;
    fiboNumbers.add(a);
    fiboNumbers.add(b);
    while(i<n)
    {
        sum = a+b;
        a=b;
        b=sum;
        fiboNumbers.add(sum);
        i++;
    }
    return fiboNumbers;
}